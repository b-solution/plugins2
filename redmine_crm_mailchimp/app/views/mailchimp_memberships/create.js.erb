$('#mailchimp-lists-placeholder').html("<%= j(render(:partial => 'mailchimp/mailchimp_lists'))%>")
$('.flash').remove()
<% if @error %>
  $('#content').prepend("<%= j("<div class='flash error' id='flash_error'>#{@error}</div>".html_safe) %>");
<% end %>
<% if @notice %>
  $('#content').prepend("<%= j("<div class='flash notice' id='flash_notice'>#{@notice}</div>".html_safe) %>");
<% end %>